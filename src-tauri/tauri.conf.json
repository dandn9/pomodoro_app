{
	"build": {
		"beforeBuildCommand": "npm run build && npm run export",
		"beforeDevCommand": "npm run dev",
		"devPath": "http://localhost:3000",
		"withGlobalTauri": true,
		"distDir": "../out"
	},
	"package": {
		"productName": "Pomodoro",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": true,
			"notification": {
				"all": true
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "com.tauri.devv",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 700,
				"minWidth": 400,
				"minHeight": 500,
				"width": 624,
				"resizable": true,
				"title": "Pomodoro",
				"decorations": false
			}
		]
	}
}
